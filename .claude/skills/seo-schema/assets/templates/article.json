{
  "@context": "https://schema.org",
  "@type": "Article",
  "headline": "{{TITLE}}",
  "description": "{{DESCRIPTION}}",
  "image": "{{IMAGE_URL}}",
  "datePublished": "{{PUBLISH_DATE}}",
  "dateModified": "{{MODIFIED_DATE}}",
  "author": {
    "@type": "Person",
    "name": "{{AUTHOR_NAME}}",
    "url": "{{AUTHOR_URL}}"
  },
  "publisher": {
    "@type": "Organization",
    "name": "{{PUBLISHER_NAME}}",
    "logo": {
      "@type": "ImageObject",
      "url": "{{PUBLISHER_LOGO}}"
    }
  },
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "{{PAGE_URL}}"
  }
}
